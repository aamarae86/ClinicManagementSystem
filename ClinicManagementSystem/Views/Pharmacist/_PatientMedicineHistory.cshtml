@model List<DAL.GetPatientMedicineHistory_Result>

@if (Model.Count > 0)
{
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Patient Name</th>
                <th>Disease</th>
                <th>Medicine</th>
                <th>Date</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.PatientName</td>
                    <td>@item.Disease</td>
                    <td>@item.Medicine</td>
                    <td>@item.Date</td>
                </tr>
            }
        </tbody>
    </table>

}
